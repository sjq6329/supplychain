  <div id="financeBox">
   <form name="finance">
    <table class="diyfFinTable" width="100%" border="0" cellspacing="1" cellpadding="0">
     <tbody>

      <tr>
        <td colspan="4" style="padding:10px 0; font-weight:bold;">财务分析(单位：元)</td>
      </tr>

      {% for item in data %}
      {% if item.item in ['项目','货币资金','应收账款','其他应收款','存货','固定资产净值','在建工程','无形／递延资产','短期借款','应付票据','其他应付款','流动负债合计','长期借款','实收资本','资本公积','盈余公积','未分配利润','所有者权益']%}
      <tr>
        <td>{{item.item}}</td>
        <td>{{item.firstYear}}</td>
        <td>{{item.currentYear}}</td>
        <td>{{item.currentMonth}}</td>
      </tr>
      {% endif %}
      {% endfor %}

      <tr>
        <td colspan="4" style="padding:10px 0; font-weight:bold;">企业成长性和盈利能力分析</td>
      </tr>

      {% for item in data %}
      {% if item.item in ['主营业收入','主营业务成本','销售费用','管理费用','财务费用','利润总额','净利润','销售利润率','净利润率'] %}
        <tr>
          <td>{{item.item}}</td>
          <td>{{item.firstYear}}</td>
          <td>{{item.currentYear}}</td>
          <td>{{item.currentMonth}}</td>
        </tr>
      {% endif %}
      {% endfor %}

      <tr>
        <td colspan="4" style="padding:10px 0; font-weight:bold;">偿债及营运能力分析</td>
      </tr>

      {% for item in data %}
      {% if item.item in ['资产负债率','利息保障倍数','流动比率','速动比率','现金比率','存货周转率'] %}
      <tr>
        <td>{{item.item}}</td>
        <td>{{item.firstYear}}</td>
        <td>{{item.currentYear}}</td>
        <td>{{item.currentMonth}}</td>
      </tr>
      {% endif %}
      {% if (loop.last) %}
      <tr>
           <td>财务数据分析补充说明</td>
           <td colspan="3">{{item.financialDataAnalysis}}</td>
        </tr>
        <tr>
          <td>企业盈利能力、成长性和偿债能力分析补充说明</td>
          <td colspan="3">{{item.profitabilityAndGrowth}}</td>
        </tr>
      {% endif %}
      {% endfor %}

     </tbody>
    </table>
   </form>
  </div>